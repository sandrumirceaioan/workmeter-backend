    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "postinstall": "start"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.9",
    "@nestjs/core": "^4.5.10",
    "@nestjs/microservices": "^4.5.8",
    "@nestjs/mongoose": "^3.0.1",
    "@nestjs/testing": "^4.5.5",
    "@nestjs/websockets": "^4.5.8",
    "@types/mongoose": "^5.0.13",
    "@types/socket.io": "^1.4.33",
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "jwt-then": "^0.6.0",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "moment": "^2.21.0",
    "mongoose": "^5.0.17",
    "mongoose-hidden": "^1.5.2",
    "mongoose-validator": "^1.3.2",
    "nest-mongoose": "^1.0.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "socket.io": "^2.0.4",
    "typescript": "^2.6.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^10.0.6",
    "@types/supertest": "^2.0.4",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^6.0.3",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
