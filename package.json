{
   "name": "workmeter-backend",
   "version": "1.0.0",
   "description": "NodeJS Nest App",
   "license": "MIT",
   "scripts": {
      "start": "node index.js",
      "start:watch": "nodemon",
      "foreman": "foreman start -f Procfile.dev",
      "prestart:prod": "tsc",
      "start:prod": "node dist/main.js",
      "migrations:generate": "node_modules/ts-node/dist/bin.js node_modules/.bin/typeorm migrations:generate",
      "migrations:run": "node_modules/ts-node/dist/bin.js node_modules/.bin/typeorm migrations:run",
      "migrations:revert": "node_modules/ts-node/dist/bin.js node_modules/.bin/typeorm migrations:revert",
      "lint": "node_modules/tslint/bin/tslint -t codeFrame 'src/**/*.ts'"
   },
   "dependencies": {
      "@nestjs/common": "^4.4.0",
      "@nestjs/core": "^4.4.0",
      "@nestjs/microservices": "^4.4.0",
      "@nestjs/swagger": "^1.1.3",
      "@nestjs/testing": "^4.4.0",
      "@nestjs/typeorm": "^2.0.0",
      "@nestjs/websockets": "^4.4.0",
      "await-to-js": "^2.0.1",
      "bcrypt": "^1.0.3",
      "class-validator": "^0.8.1",
      "cors": "^2.8.4",
      "dotenv": "^4.0.0",
      "express": "^4.16.2",
      "jsonwebtoken": "^8.1.0",
      "morgan": "^1.9.0",
      "passport": "^0.4.0",
      "passport-jwt": "^3.0.1",
      "pg": "^7.3.0",
      "redis": "^2.7.1",
      "reflect-metadata": "^0.1.10",
      "request": "^2.83.0",
      "rxjs": "^5.4.0",
      "ts-node": "~3.3.0",
      "typeorm": "0.1.11",
      "typescript": "~2.7.0"
   },
   "devDependencies": {
      "@types/bcrypt": "^1.0.0",
      "@types/express": "^4.11.0",
      "@types/jsonwebtoken": "^7.2.3",
      "@types/node": "^8.0.29",
      "@types/passport-jwt": "^3.0.0",
      "nodemon": "^1.12.1",
      "tslint": "^5.8.0"
   }
}
